[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  commend = commit --amend
[core]
  excludesFile = /home/{{ item.name }}/.gitignore_global
  editor = vim
[user]
  email = {{ item.email }}
  name = {{ item.name }}
[credential]
  helper = store
